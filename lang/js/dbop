var dbname = argv.dbname || "main";
var params = {};
params.method = "db." + dbname + "." + $.eval(argv.method);
params.args = ['"'+$.eval(argv.schema)+'"'];
if(argv.arg1)
	params.args.push($.eval(argv.arg1));
if(argv.arg2)
	params.args.push($.eval(argv.arg2));
params.returns = [];
if(argv.result){
	var tmp = {};tmp[argv.result] = "json";
	params.returns.push(tmp);
}
$.extend1(params, parent);
