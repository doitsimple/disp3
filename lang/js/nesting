var funcs;
if(!$.isArray(argv)){
 funcs = [argv];
}else{
 funcs = argv;
}
var content = "";
for(var i = funcs.length-1;i>=0;i--){
 funcs[i].error = $.concat(funcs[i].error, parent.error);
 funcs[i].success = $.concat(funcs[i].success, content);
 if(i == funcs.length-1)
 	 funcs[i].success = $.concat(funcs[i].success, parent.success);
 content = $.eval(funcs[i]);
}
