var funcs;
if(!$.isArray(argv)){
 funcs = [argv];
}else{
 funcs = argv;
}
var content = "";
for(var i = funcs.length-1;i>=0;i--){
 funcs[i].error = $.concat(parent.error, funcs[i].error);
 funcs[i].success = $.concat(parent.success, funcs[i].success, content);
// funcs[i].content = $.eval(funcs[i].content) + content;
 content = $.eval(funcs[i]);
}
