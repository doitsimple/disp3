var funcs;
if(!$.isArray(argv)){
 funcs = [argv];
}else{
 funcs = argv;
}
var content = "";
for(var ii = funcs.length-1;ii>=0;ii--){
 if(!funcs[ii]) throw $.stringify(parent)
 funcs[ii].error = $.concat(funcs[ii].error, parent.error);
 funcs[ii].success = $.concat(funcs[ii].success, content);
 if(ii == funcs.length-1)
 	 funcs[ii].success = $.concat(funcs[ii].success, parent.success);

 content = $.eval(funcs[ii]);
}
