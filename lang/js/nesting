var funcs;
if(!$.isArray(argv)){
 funcs = [argv];
}else{
 funcs = argv;
}
var content = "";
for(var i = funcs.length-1;i>=0;i--){
 if(!funcs[i].content) funcs[i].content = "";
 if(!funcs[i].error) funcs[i].error = "";
 if(!funcs[i].success) funcs[i].success = "";
 if(parent.error){
 	funcs[i].error += $.eval(parent.error) + $.eval(funcs[i].error);;
 }
 funcs[i].content += $.eval(funcs[i].content) + content;
 content = $.eval(funcs[i]);
}
$$
