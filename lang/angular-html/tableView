^^local.makeng = function(sube){
	if(!sube) return "";
  var addonstr = "";
  if(sube.click)
  addonstr += 'ng-click="' + sube.click + '" ';
  if(sube.class)
  addonstr += 'ng-class="' + sube.class + '" ';
  if(sube.repeat)
  addonstr += 'ng-repeat="' + sube.repeat + '" ';
  if(sube.href)
  addonstr += 'ng-href="' + sube.href + '" ';
  if(sube.show)
  addonstr += 'ng-show="' + sube.show + '" ';
  if(sube.model)
	addonstr += 'ng-model="' + sube.model + '" ';
	if(sube.attr)
	addonstr += sube.attr + ' ';
	return addonstr;
}$$
